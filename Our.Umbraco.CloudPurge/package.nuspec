<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>CloudPurge</id>
		<version>$version$</version>
		<title>CloudPurge for Umbraco</title>
		<authors>Anthony Halliday</authors>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<summary>CloudPurge: Purge CDN cache from Umbraco</summary>
		<description>Clear CDN cache for Umbraco on content save</description>
		<iconUrl>https://raw.githubusercontent.com/anth12/CloudPurge/master/logo.png</iconUrl>
		<releaseNotes>
			v0.0.5    - Backoffice Content Type picker
			v0.0.4    - PublishHookEnabled switch
			v0.0.3    - Verbose logging
			v0.0.2    - Target Umbraco 8.5.5
			v0.0.1    - Initial release
		</releaseNotes>
		<repository type="git" url="https://github.com/anth12/CloudPurge" />
		<dependencies>
			 <dependency id="UmbracoCms.Core" version="8.5.5" /> 
		</dependencies>
	</metadata>

	<files>
		<file src="..\Our.Umbraco.CloudPurge.Website\App_Plugins\**\*.*" target="content\App_Plugins\" />
	</files>
</package>